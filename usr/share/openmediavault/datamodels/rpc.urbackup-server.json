[{
	"type": "rpc",
	"id": "rpc.urbackup-server.setsettings",
	"params": {
		"type": "object",
		"properties": {
		    "enable": {
		        "type": "boolean",
		        "required": true
		    },
			"port": {
			    "type": "integer",
			    "minimum": 1,
				"maximum": 65535,
		        "required": true
			},
			"webport": {
			    "type": "integer",
			    "minimum": 1,
				"maximum": 65535,
		        "required": true
			},
			"loglevel": {
			    "type": "string",
				"enum":[ "error", "warn", "info", "debug" ],
		        "required": true
			},
			"runasroot": {
		        "type": "boolean",
		        "required": false
		    }
		}
	}
}]
