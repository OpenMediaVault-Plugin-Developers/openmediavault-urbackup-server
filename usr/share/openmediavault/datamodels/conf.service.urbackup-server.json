{
	"type": "config",
	"id": "conf.service.urbackup-server",
	"title": "UrBackup Server",
	"queryinfo": {
		"xpath": "//services/urbackup-server",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": 0
		},
		"port": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 55413
		},
		"webport": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 55414
		},
		"loglevel": {
			"type": "string",
			"default": "warn"
		},
		"runasroot": {
			"type": "boolean",
			"default": 0
		}
	}
}
